// default options used for generate classes
$rs-options = {
  // global prefix that used as namespace
  'global-prefix': 'rs',

  // margin and padding setting
  'margin-padding': {
    '1x': 8px,
    '30': 30px,
    'sectionA': 10px 20px 10px,
  },

  // color setting
  'color': {
    my-red: red,
    my-blue: rgb(0, 0, 255),
    my-green: green,
  },

  // transition setting
  'transition': {
    default: all 300ms ease,
    my-color: color 200ms,
  },

  // settings of media query class
  'media-query': {
    sm: (null 599),
  }
};
