@import '../../_mixins';


/**
 * Mixin to create classes of overflow
 */
uc-overflow($prefix = '') {
  uc-generate-classes-by-option({
    prefix: ($prefix 'overflow'),
    property: overflow,
    data: {
      visible: visible,
      scroll: scroll,
      hidden: hidden,
      auto: auto,
    },
  });
}
